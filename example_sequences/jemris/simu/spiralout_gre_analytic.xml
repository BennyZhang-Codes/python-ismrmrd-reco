<?xml version="1.0" encoding="utf-8"?>
<Parameters FOVx="222" FOVy="222" FOVz="1" GradMaxAmpl="13" GradSlewRate="33" Name="P" Nx="148" Ny="148" Nz="1" TE="5" TR="2000">
   <ConcatSequence Aux1="0" LoopFlag="1" Name="Seq" Repetitions="1">
      <CONCATSEQUENCE LoopFlag="2" Name="Intl" Repetitions="15">
         <ATOMICSEQUENCE Name="A1">
            <HARDRFPULSE Duration="0.1" FlipAngle="30" InitialPhase="PhsInc*IL*(IL+1)" Name="P1" Observe="IL=Intl.Counter, PhsInc=Seq.Aux1" Symmetry="0.5"/>
         </ATOMICSEQUENCE>
         <DELAYATOMICSEQUENCE Delay="TE" DelayType="C2E" Name="TE" Observe="TE=P.TE" StartSeq="A1" StopSeq="TE"/>
         <ATOMICSEQUENCE Aux1="1" Aux2="KMAX/(DK*R)" Name="A2" Observe="KMAX=P.KMAXx, DK=P.DKx, R=Intl.Repetitions, IL=Intl.Counter" RotAngle="360*IL/R">
            <ANALYTICGRADPULSE ADCFlag="2" ADCs="2000" Axis="GX" Diff="1" Duration="10" Name="SPox" Observe="KMAX=P.KMAXx, ALPHA=A2.Aux1, TURNS=A2.Aux2, DUR=SPox.Duration" PhaseLock="1" Shape="KMAX*ALPHA*T/DUR/sqrt(ALPHA+(1-ALPHA)*T/DUR)*cos(2*Pi*TURNS*T/DUR/sqrt(ALPHA+(1-ALPHA)*T/DUR))"/>
            <ANALYTICGRADPULSE Axis="GY" Diff="1" Duration="DUR" Name="SPoy" Observe="KMAX=P.KMAXx, ALPHA=A2.Aux1, TURNS=A2.Aux2, DUR=SPox.Duration" Shape="KMAX*ALPHA*T/DUR/sqrt(ALPHA+(1-ALPHA)*T/DUR)*sin(2*Pi*TURNS*T/DUR/sqrt(ALPHA+(1-ALPHA)*T/DUR))"/>
         </ATOMICSEQUENCE>
         <ATOMICSEQUENCE Name="A3" Observe="R=Intl.Repetitions, IL=Intl.Counter" RotAngle="360*IL/R">
            <TRAPGRADPULSE Area="-A" Axis="GX" Name="Rephx" Observe="A=SPox.Area"/>
            <TRAPGRADPULSE Area="-A" Axis="GY" Name="Rephy" Observe="A=SPoy.Area"/>
         </ATOMICSEQUENCE>
         <DELAYATOMICSEQUENCE Delay="TR" DelayType="B2E" Name="TR" Observe="TR=P.TR" StartSeq="A3"/>
      </CONCATSEQUENCE>
   </ConcatSequence>
</Parameters>